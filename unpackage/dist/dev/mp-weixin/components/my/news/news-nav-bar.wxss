
.nav-left>view,
.nav-right>view {
  font-size: 40rpx;
}
.nav-left {
  width: 80%;
  color: #ff9619;
}
.nav-right {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.nav-tab-bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 100%;
}
.nav-tab-bar view {
  padding: 0 10rpx;
  font-weight: bold;
  color: #969696;
}
.active {
  color: #333333 !important;
}
.nav-tab-bar view.active .nav-tab-bar-line {
  border-bottom: 5rpx solid #FEDE33;
  border-radius: 20rpx;
  border-top: 5rpx solid #FEDE33;
}

